      "bundleIdentifier": "com.nicoaraujo.inthedark",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#1a1a1a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nicoaraujo.inthedark"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "404238d5-6ed1-41c3-b7a4-8e76d28914d2"
      }
    },
    "owner": "stout6370"
  }
